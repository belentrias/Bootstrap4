{
  "name": "bootstrap4",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "concurrently \" npm run watch:scss \" \" npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin imagenes/* --out-dir dist/imagenes",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin precios.html -d dist --htmlmin -o dist/precios.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://BelenTrias@bitbucket.org/BelenTrias/bootstrap4.git"
  },
  "author": "Belén Trías",
  "license": "ISC",
  "homepage": "https://bitbucket.org/BelenTrias/bootstrap4#readme",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "imagemin": "^7.0.1",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
